{
  "name": "game-socket",
  "type": "module",
  "scripts": {
    "watch": "nodemon --ext js,ts,json,env --exec 'npm run build'",
    "build": "tsc --project tsconfig.json",
    "build:clean": "rm -rf tsconfig.tsbuildinfo && rm -rf ./dist",
    "format": "prettier \"./**\" --write --ignore-unknown",
    "format:check": "prettier \"./**\" --ignore-unknown --check"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "redux-actions": "^2.6.5",
    "rpc-websockets": "^7.5.0",
    "tslib": "^2.3.1",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/redux-actions": "^2.6.2",
    "@types/ws": "^8.5.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
